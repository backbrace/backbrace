{
  "root": true,
  "extends": "../../../.eslintrc-browser.json",
  "env": {
    "jasmine": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-invalid-this": "off",
    "no-throw-literal": "off",
    "no-unused-vars": "off"
  },
  "globals": {
    "window": false,
    "backbrace": false
  }
}